@{
    Layout = "~/Views/Shared/_Layoutct.cshtml";
}

@section main{

    <h3>Cadastro</h3>

    <div class="mb-3 w-75">
        <label for="" class="form-label">Telefone Pessoal*</label>
        <input type="text" name="" id="" class="form-control" placeholder="" aria-describedby="helpId">
        <label for="" class="form-label">CEP*</label>
